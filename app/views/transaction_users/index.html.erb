<p id="notice"><%= notice %></p>

<h1>Transaction Users</h1>

<table>
  <thead>
    <tr>
      <th>Point</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transaction_users.each do |transaction_user| %>
      <tr>
        <td><%= transaction_user.point %></td>
        <td><%= link_to 'Show', transaction_user %></td>
        <td><%= link_to 'Edit', edit_transaction_user_path(transaction_user) %></td>
        <td><%= link_to 'Destroy', transaction_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction User', new_transaction_user_path %>
